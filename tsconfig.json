{
  "compilerOptions": {
    "lib": ["ES2020"],
    "target": "es5",
    "module": "commonjs",
    "allowJs": false,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "typeRoots": ["node_modules/@types", "typings"]
  }
}
